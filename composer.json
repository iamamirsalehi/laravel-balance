{
    "name": "iamamirsalehi/laravel-balance",
    "description": "Manage your users balance in your website ",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Amir Salehi",
        "email": "iamamirsalehi@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "Iamamirsalehi\\LaravelBalance\\": "src/",
            "Iamamirsalehi\\LaravelBalance\\Database\\Factories\\": "database/factories"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Iamamirsalehi\\LaravelBalance\\": "tests/"
        }
    },

    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0"
    },

    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iamamirsalehi\\LaravelBalance\\LaravelBalanceServiceProvider"
            ]
        }
    }
}